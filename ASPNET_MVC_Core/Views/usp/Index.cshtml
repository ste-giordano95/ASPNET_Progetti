@model IEnumerable<ASPNET_MVC_Core.Models.TenMostExpensiveProductsResult>
@{
    ViewData["Title"] = "Index";
}

<h1>SP</h1>
<table class="table">
    <thead>
        <tr>

            <th scope="col">Nome Prodotto</th>
            <th scope="col">Costo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.TenMostExpensiveProducts</td>
                <td>@item.UnitPrice</td>

            </tr>

        }
    </tbody>
</table>
